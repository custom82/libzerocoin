prefix=@prefix@
exec_prefix=@exec_prefix@
libdir=@libdir@
includedir=@includedir@

Name: libzerocoin
Description: Zerocoin cryptographic library
Version: @PACKAGE_VERSION@
Requires: openssl >= 3.0.0, gmp >= 6.3.0
Libs: -L${libdir} -lzerocoin
Cflags: -I${includedir}/zerocoin -I${includedir}/zerocoin/bitcoin_bignum -DOPENSSL_API_COMPAT=0x30000000L
